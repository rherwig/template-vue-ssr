{
  "name": "template-vue-ssr",
  "version": "1.0.0",
  "description": "Server side rendering template for VueJS",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node src/index",
    "start:prod": "cross-env NODE_ENV=production babel-node src/index",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client/prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server/prod.js",
    "build:node": "cross-env NODE_ENV=production webpack --config webpack/node.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rherwig/template-vue-ssr.git"
  },
  "keywords": [
    "ssr",
    "vue",
    "universal",
    "express",
    "pwa"
  ],
  "author": "Rico Herwig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rherwig/template-vue-ssr/issues"
  },
  "homepage": "https://github.com/rherwig/template-vue-ssr#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-loader": "^8.0.5",
    "chokidar": "^2.1.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "memory-fs": "^0.4.1",
