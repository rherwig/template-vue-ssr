{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current",
        "browsers": ["last 2 versions", "IE 11"]
      },
      "useBuiltIns": "usage",
      "corejs": {
        "version": 3,
        "proposals": true
      }
    }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ]
}
